var img;
img = new SimpleImage("skyline.jpg"); // The image is optional

for(var pixel of img.values()){ //For Loop counting the Pixels
    
    if(pixel.getX() <= 100){ //Getting the first third of the width of the image  
        pixel.setRed(255);  //Applying my modifications
    }
    else if (pixel.getX() > 100 && pixel.getX() <= 200){ //Getting the second third of the width of the image  
        pixel.setGreen(255);  //Applying my modifications
    }
     else if (pixel.getX() > 200 && pixel.getX() <= 300){ //Getting the last third of the width of the image  
        pixel.setBlue(255); //Applying my modifications
}
    
}
print(img);
